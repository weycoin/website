  <div class="container">
    <div class="row">
      <div class="col-lg-12 text-center">
        <h2 class="section-heading text-uppercase">Our mining pools</h2>
        <h3 class="section-subheading text-muted">Got some power? Use it for da WAE!</h3>
      </div>
    </div>
    <div class="row text-center justify-content-center">

      <div class="col-md-4">
        <h4 class="service-heading">GoHashMe</h4>
        <a class="btn btn-info btn-xl text-uppercase js-scroll-trigger" href="http://gohashme.com/" target="_blank">gohashme.com</a>
      </div>

      <div class="col-md-4">
        <h4 class="service-heading">ALIEN</h4>
        <a class="btn btn-info btn-xl text-uppercase js-scroll-trigger" href="http://alien-mining-pool.com/" target="_blank">alien-mining-pool.com</a>
      </div>
      
      <div class="col-md-4">
        <h4 class="service-heading">RarePool Pool</h4>
        <a class="btn btn-info btn-xl text-uppercase js-scroll-trigger" href="http://rarepool.com/" target="_blank">rarepool.com</a>
      </div>

    </div>
  </div>
</section>